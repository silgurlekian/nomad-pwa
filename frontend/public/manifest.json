  "id": "nomad",
  "description": "La app que centraliza todos los espacios de trabajo en tu ciudad",
  "dir": "ltr",
  "lang": "es",
  "categories": [
    "lifestyle"
  ],
  "display_override": [
    "window-controls-overlay"
  ],
  "handle_links": {
    "enabled": true,
    "action": "/link-handler",
    "accept": [
      "https://www.nomad.com/spaces/*",
      "https://nomad.com/book/*"
    ]
  },
  "protocol_handlers": [
    {
      "protocol": "nomad",
      "url": "/link-handler?url=%s"
    }
  ],
  "share_target": {
    "action": "/share-handler",
    "accept": [
      "image/*",
      "text/*"
    ]
  },
  "background_sync": {
    "enabled": true
  },
  "periodic_sync": {
    "enabled": true,
    "interval": 86400000
  },
  "push": {
    "enabled": true
  }
}
